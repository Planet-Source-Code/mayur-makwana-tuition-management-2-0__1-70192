Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
Select Case KeyCode
Case vbKeyF2
    frmInq.Show
Case vbKeyF3
    frmCust.Show
Case vbKeyF4
    frmComp.Show
Case vbKeyF6
    frmReports.Show
Case vbKeyF7
    frmAccount.Show
Case vbKeyF8
    txtname.Show
Case vbKeyF9
        frmSplash.Show
        frmSplash.Timer1.Enabled = False
End Select
End Sub

Private Sub Form_Resize()
Me.WindowState = 2
End Sub

Private Sub Toolbar1_ButtonClick(ByVal Button As MSComctlLib.Button)
Select Case Button.Index
    Case 1
        frmInq.Show
    Case 2
        frmCust.Show
    Case 3
        frmComp.Show
    Case 5
        frmUsers.Show
    Case 6
        frmReports.Show
    Case 7
        frmAccount.Show
    Case 8
        txtname.Show
    Case 9
        frmSplash.Show
        frmSplash.Timer1.Enabled = False
End Select
End Sub

